{
    "name": "poptin/poptin",
    "description": "Create advanced website popups and forms, and improve your website's conversion rate within a few minutes.",
    "type": "craft-plugin",
    "version": "2.0.6",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "poptin"
    ],
    "support": {
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Poptin",
            "homepage": "https://poptin.com"
        }
    ],
    "require": {
        "craftcms/cms": "^4.0"
    },
    "require-dev": {
        "craftcms/rector": "dev-main"
    },
    "autoload": {
        "psr-4": {
          "poptin\\poptin\\": "src/"
        }
    },
    "extra": {
        "name": "Poptin",
        "handle": "poptin",
        "developer": "Poptin",
        "developerUrl": "poptin.com",
        "documentationUrl": "https://github.com/poptins/craftcms-poptin-app/blob/master/README.md",
        "changelogUrl": "https://github.com/poptins/craftcms-poptin-app/blob/master/CHANGELOG.md",
        "class": "poptin\\poptin\\Poptin"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
