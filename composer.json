{
    "name": "poptin/poptin",
    "description": "Create advanced website popups and forms, and improve your website's conversion rate within a few minutes.",
    "type": "craft-plugin",
    "version": "1.18.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "poptin"
    ],
    "support": {
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Poptin",
            "homepage": "https://poptin.com"
        }
    ],
    "require": {
        "craftcms/cms": "^3.6.7"
    },
    "require-dev": {
      "craftcms/cms": "^3.6.7",
      "codeception/codeception": "^4.1",
      "vlucas/phpdotenv": "^3.0"
    },
    "autoload": {
        "psr-4": {
          "poptin\\poptin\\": "src/"
        }
    },
    "extra": {
        "name": "Poptin",
        "handle": "poptin",
        "developer": "Poptin",
        "developerUrl": "poptin.com",
        "documentationUrl": "https://github.com/poptins/craftcms-poptin-app/blob/master/README.md",
        "changelogUrl": "https://github.com/poptins/craftcms-poptin-app/blob/master/CHANGELOG.md",
        "class": "poptin\\poptin\\Poptin"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
